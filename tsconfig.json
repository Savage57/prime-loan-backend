{
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "module": "commonjs",
      "target": "es6",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*", "src/workers"],
    "exclude": ["node_modules"]
}